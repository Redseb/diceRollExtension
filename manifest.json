{
    "manifest_version": 2,
    "name": "Roll Dice!",
    "version": "0.1",
    "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "js": ["content.js"]
      }
    ],
    "browser_action": {
      "default_icon": "d6.png",
      "default_popup": "popup.html",
      "default_title": "Roll dice!"
    }
  }